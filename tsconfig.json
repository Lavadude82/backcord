{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@models/*": ["src/database/models/*"],
      "@db/*": ["src/database/*"],
      "@controllers/*": ["src/controllers/*"],
      "@dto/*": ["src/controllers/dto/*"],
      "@interface/*": ["src/interfaces/*"],
      "@repo/*": ["src/repositories/*"],
      "@routes/*": ["src/routes/*"],
      "@middleware/*": ["src/middlewares/*"],
      "@utils/*": ["src/utils/*"],
      "@utils/regex": ["src/utils/Regexes"],
      "@conftype": ["src/extra/conf.type"],
      "@conf": ["config/.conf"]
    },
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false
  }
}
